{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "ES2017"
    ],
    "module": "ES2015",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ES2017"
  }
}
