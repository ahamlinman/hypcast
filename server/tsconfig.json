{
  "compilerOptions": {
    "alwaysStrict": true,
    "lib": [
      "DOM",
      "ES2018"
    ],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../dist/server/",
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "ES2018"
  }
}
